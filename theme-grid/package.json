{
  "name": "theme-grid",
  "homepage": "https://storybook.keycloakify.dev",
  "version": "0.0.2",
  "description": "A starter/demo project for keycloakify",
  "repository": {
    "type": "git",
    "url": "git://github.com/garronej/keycloakify-storybook.git"
  },
  "scripts": {
    "postinstall": "copy-keycloak-resources-to-public",
    "start": "react-scripts start",
    "build": "CI=false && react-scripts build",
    "build-keycloak-theme": "yarn build && keycloakify",
    "storybook": "storybook dev -p 6006"
  },
  "keycloakify": {
    "themeName": "theme-grid",
    "extraThemeProperties": [
      "foo=bar"
    ]
  },
  "keywords": [],
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "evt": "^2.5.7",
    "framer-motion": "^10.18.0",
    "keycloakify": "^9.1.9",
    "lucide-react": "^0.314.0",
    "oidc-spa": "^3.0.3",
    "powerhooks": "^1.0.8",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsafe": "^1.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/node-logger": "^7.6.10",
    "@storybook/preset-create-react-app": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^15.3.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "react-scripts": "5.0.1",
    "storybook": "^7.6.10",
    "typescript": "~4.7.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/no-redeclare": "off",
      "no-labels": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
